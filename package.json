{
  "name": "deploy-this",
  "version": "1.0.0",
  "description": "A CLI tool for automating AWS infrastructure deployment and Bitbucket pipeline configuration",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "iodt": "dist/index.js"
  },
  "scripts": {
    "build:templates": "copyfiles -a -u 1 src/templates/environments/.* dist/ ; copyfiles -a -u 1 src/templates/terraforms/**/* dist/",
    "chmod:bin": "chmod-cli ./dist/index.js -m 0o755",
    "build": "rimraf ./dist/ && tsc && npm run chmod:bin && npm run build:templates",
    "test": "jest --coverage=true --cache=false --runInBand=true --forceExit --detectOpenHandles",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "debug": "ts-node src/index.ts",
    "dev": "npm run build && ts-node src/index.ts",
    "start": "node dist/index.js",
    "watch": "tsc -w",
    "tf:init": "cd src/templates/terraforms && terraform init",
    "tf:plan": "cd src/templates/terraforms && terraform plan",
    "tf:apply": "cd src/templates/terraforms && terraform apply",
    "tf:destroy": "cd src/templates/terraforms && terraform destroy",
    "prepack": "npm run build",
    "postinstall": "npm run build"
  },
  "keywords": [
    "aws",
    "infrastructure",
    "deployment",
    "bitbucket",
    "pipeline",
    "automation"
  ],
  "author": "Dimas Widianto <dimas@imaginaryones.com>",
  "contributors": [
    "Arif Widianto <https://github.com/sikancil>"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.6.1",
    "prettier": "^3.3.3",
    "prettier-plugin-sh": "^0.14.0",
    "prettier-plugin-toml": "^2.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.679.0",
    "@aws-sdk/client-ecr": "^3.370.0",
    "@aws-sdk/client-iam": "^3.677.0",
    "@types/lodash": "^4.17.12",
    "@types/node": "^20.17.0",
    "@types/prompts": "^2.4.9",
    "axios": "^1.4.0",
    "chmod-cli": "^2.0.1",
    "commander": "^11.0.0",
    "compare-versions": "^6.1.1",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "prompts": "^2.4.2",
    "rimraf": "^4.4.1",
    "typescript": "^5.1.6"
  }
}
